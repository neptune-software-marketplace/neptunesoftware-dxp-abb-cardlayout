				{
					"fieldNo": "f24c2e23-bee3-4206-96e3-00d58166aa0d",
					"fieldName": "TileLayoutGenerator",
					"fieldParent": 99999,
					"fieldType": "neptune.Script",
					"script": [
						"==OBJECT STRING==",
						"// dataRecords should have all the records as array of JSON\r",
						"// call the function generateCards on the ajaxSuccess event of the API call\r",
						"// expected structure of dataRecords:\r",
						"// {\r",
						"//     \"id\" : record_id\r",
						"//     \"Description\": Description of the Item\r",
						"//     \"SKU\" : \"short Information of Item\"\r",
						"//     \"Price\" : \"Price of Item\"\r",
						"//     \"Currency\": \"Unit of Price\"\r",
						"// }\r",
						"\r",
						"\r",
						"// Global Variables\r",
						"var numSections = 0;\r",
						"var maxTiles = 3;           // --> Number of cards per row - can be changed\r",
						"\r",
						"var defaultSize = Math.ceil(100 / maxTiles);\r",
						"var tileWidth = defaultSize;\r",
						"var oBlockLayoutRow = \"\";\r",
						"\r",
						"function generateCards(dataRecords) {\r",
						"    \r",
						"    oBlockLayout.removeAllContent();\r",
						"\r",
						"    if (dataRecords.length < 1) {\r",
						"        noRecordBanner.setVisible(true);\r",
						"    } else {\r",
						"        noRecordBanner.setVisible(false);\r",
						"    }\r",
						"\r",
						"    $.each(dataRecords, function(i, data) {\r",
						"        \r",
						"        // Create new row based on calculations\r",
						"        if (numSections === 0) {\r",
						"            oBlockLayoutRow = new sap.ui.layout.BlockLayoutRow(\"_nepBlockRow\" + ModelData.genID(), {});\r",
						"            oBlockLayout.addContent(oBlockLayoutRow);\r",
						"\r",
						"        }\r",
						"\r",
						"        numSections = numSections + (maxTiles / 100) * tileWidth;\r",
						"        \r",
						"        // One Cell\r",
						"        var oBlockLayoutCell = new sap.ui.layout.BlockLayoutCell(\"nep_\" + data.id, {\r",
						"            // width: tileWidth,\r",
						"            // title: 'Title'\r",
						"        });\r",
						"        \r",
						"        // Parent Container\r",
						"        var blockParent = new sap.m.HBox(\"nep_\" + ModelData.genID(), {});\r",
						"        \r",
						"        // Image Container\r",
						"        var blockContentImage = new sap.m.VBox(\"nep_\" + ModelData.genID(), {\r",
						"            // width: \"10%\"\r",
						"        });\r",
						"        \r",
						"        // Image\r",
						"        var oImage = new sap.m.Image(\"nep_\" + ModelData.genID(), {\r",
						"            width: \"5em\",\r",
						"            src: data.Image,            //--> URL of Image comes in field 'Image' - can be changed\r",
						"            mode: \"Background\",\r",
						"            height: \"5em\",\r",
						"            backgroundSize: \"3em\",\r",
						"            backgroundPosition: \"center\"\r",
						"        });\r",
						"        \r",
						"        // Product Information Container\r",
						"        var blockContentInfoParent = new sap.m.VBox(\"nep_\" + ModelData.genID(), {\r",
						"            // width: \"90%\"\r",
						"            justifyContent: \"SpaceBetween\"\r",
						"        });\r",
						"        \r",
						"        // Product Descriptions\r",
						"        var blockContentInfo = new sap.m.VBox(\"nep_\" + ModelData.genID(), {\r",
						"            // width: \"90%\"\r",
						"        });\r",
						"        \r",
						"        // Product Info\r",
						"        var txtInfo = new sap.m.Text(\"nep_\" + ModelData.genID(), {\r",
						"            text: data.SKU              //--> Product Shot Information field - can be changed\r",
						"        });\r",
						"        \r",
						"        // Product Description\r",
						"        var txtDescription = new sap.m.Title(\"nep_\" + ModelData.genID(), {\r",
						"            // titleStyle: \"auto\",\r",
						"            text: data.Description,     //--> Product Description field - can be changed\r",
						"            wrapping: true\r",
						"        });\r",
						"        \r",
						"        // Numeric Container\r",
						"        var blockContentValue = new sap.m.HBox(\"nep_\" + ModelData.genID(), {\r",
						"            justifyContent: \"End\"\r",
						"        });\r",
						"        // Number value - can be used for Price / Quantity / Amount\r",
						"        var txtValue = new sap.m.ObjectNumber(\"oObjectNumber\", {\r",
						"            unit: data.Currency,        //--> Unit field\r",
						"            state: \"Warning\",\r",
						"            number: data.Price          //--> Numeric field\r",
						"        });\r",
						"        \r",
						"        \r",
						"        // integrating altogther\r",
						"        oBlockLayout.addContent(oBlockLayoutRow);\r",
						"        oBlockLayoutRow.addContent(oBlockLayoutCell);\r",
						"        oBlockLayoutCell.addContent(blockParent);\r",
						"\r",
						"        blockParent.addItem(blockContentImage);\r",
						"        blockParent.addItem(blockContentInfoParent);\r",
						"\r",
						"\r",
						"        blockContentImage.addItem(oImage);\r",
						"\r",
						"        blockContentInfoParent.addItem(blockContentInfo);\r",
						"\r",
						"        blockContentInfo.addItem(txtInfo);\r",
						"        blockContentInfo.addItem(txtDescription);\r",
						"        blockContentInfoParent.addItem(blockContentValue);\r",
						"\r",
						"\r",
						"        blockContentValue.addItem(txtValue);\r",
						"        \r",
						"        \r",
						"        // Manage CSS\r",
						"        oBlockLayoutCell.addStyleClass(\"nepTileCards\");\r",
						"        oBlockLayoutCell.addStyleClass(\"nepTileClickable\");\r",
						"        oBlockLayoutCell.addStyleClass(\"tileWidth\");\r",
						"        oBlockLayoutCell.addStyleClass(\"sapUiNoContentPadding\");\r",
						"        blockParent.addStyleClass(\"sapUiTinyMargin\");\r",
						"        txtValue.addStyleClass(\"NumberSize\");\r",
						"\r",
						"        \r",
						"        // Card Actions\r",
						"        oBlockLayoutCell.attachBrowserEvent(\"click\", function(e) {\r",
						"            // butStartFn(e);\r",
						"            // var id = oBlockLayoutCell.getId();\r",
						"            onCardClick(data);          //--> click function - data will have the item record\r",
						"            // console.log(\"clicked...\");\r",
						"        });\r",
						"\r",
						"        \r",
						"        // Increment calculations -- DO NOT CHANGE\r",
						"        if (numSections >= maxTiles) numSections = 0;\r",
						"\r",
						"    });\r",
						"}"
					],
					"request": [],
					"response": [],
					"attributes": [],
					"data": []
				},
				{
					"fieldNo": "f9e1ba6f-06ae-42fa-85ab-cd4baac50c0d",
					"fieldName": "TileLayoutActions",
					"fieldParent": 99999,
					"fieldType": "neptune.Script",
					"script": [
						"==OBJECT STRING==",
						"function onCardClick(itemData) {",
						"    ",
						"    // itemData will have all the field values. use as required",
						"    // Use MessageToast",
						"    sap.m.MessageToast.show(\"Product Clicked-\"+ itemData.Description);",
						"    // console.log(\"Product Clicked-\"+ itemData.Description);",
						"}"
					],
					"request": [],
					"response": [],
					"attributes": [],
					"data": []
				}
			]
