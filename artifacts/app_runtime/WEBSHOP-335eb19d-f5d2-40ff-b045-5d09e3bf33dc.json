				{
					"fieldNo": "96747cfc-5386-4a62-b1b7-81dac553b02f",
					"fieldName": "How-To-Use",
					"fieldParent": 99999,
					"fieldType": "neptune.Script",
					"script": [
						"==OBJECT STRING==",
						"// How To Use",
						"// Call the function generateCards(dataRecords) to create Cards",
						"// dataRecords - Array (of JSON) of the records - Product List / Order List",
						"// expected structure of dataRecords:",
						"// {",
						"//     \"id\" : record_id",
						"//     \"Description\": Description of the Item",
						"//     \"SKU\" : \"short Information of Item\"",
						"//     \"Price\" : \"Price of Item\"",
						"//     \"Currency\": \"Unit of Price\"",
						"// }",
						"",
						"",
						"// The Structure can be different with different field names, this requires",
						"//adjustments in the function generateCards for txtInfo, txtDescription, oImage and txtValue",
						"",
						"",
						"// the Cards will be added to oBlockLayout (contained inside a sap.m.Panel)",
						"",
						""
					],
					"request": [],
					"response": [],
					"attributes": [],
					"data": []
				},
				{
					"fieldNo": "02ea0f78-e21b-4aa6-a009-524a27cfa9e2",
					"fieldName": "oHTMLObject",
