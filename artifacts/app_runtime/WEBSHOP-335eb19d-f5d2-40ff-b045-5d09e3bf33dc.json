					"data": [
						{
							"fieldNo": "689894f2-f664-4a25-c7bc-6194640eaa9f",
							"fieldName": "TileLayoutCSS",
							"fieldParent": "02ea0f78-e21b-4aa6-a009-524a27cfa9e2",
							"fieldType": "neptune.Html",
							"script": [
								"==OBJECT STRING==",
								"<style>",
								"    .NumberSize >.sapMObjectNumberText {",
								"        font-size: 1.5rem !important;",
								"    }",
								"",
								"    .nepTileCards {",
								"        margin: 8px 8px 8px 8px;",
								"        box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16), 0 3px 6px rgba(0, 0, 0, 0.23);",
								"        border-radius: 10px;",
								"    }",
								"",
								"    .nepTileCards:hover {",
								"        box-shadow: 0 10px 24px 0 rgba(0, 0, 0, 0.2);",
								"    }",
								"",
								"    .nepTileClickable {",
								"        cursor: pointer;",
								"    }",
								"",
								"    /* Extra small devices (phones, 600px and down) */",
								"",
								"    @media only screen and (max-width: 600px) {",
								"        .tileWidth {",
								"            width: 100% !important",
								"        }",
								"    }",
								"",
